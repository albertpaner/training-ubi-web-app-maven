<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:web="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <display-name>UbiWebApp</display-name>
  <servlet>
    <servlet-name>AdderServlet</servlet-name>
    <servlet-class>servlets.AdderServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AdderServlet</servlet-name>
    <url-pattern>/add2</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>RegistrationServlet</servlet-name>
    <servlet-class>servlets.RegistrationServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RegistrationServlet</servlet-name>
    <url-pattern>/register</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>servlets.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>DistributeEvaluatorsServlet</servlet-name>
    <servlet-class>servlets.DistributeEvaluatorsServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DistributeEvaluatorsServlet</servlet-name>
    <url-pattern>/distribute_evaluators</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>CreateValServlet</servlet-name>
    <servlet-class>servlets.CreateValServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CreateValServlet</servlet-name>
    <url-pattern>/visualizza_val</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>UtenteGiovane</display-name>
    <servlet-name>UtenteGiovane</servlet-name>
    <servlet-class>servlets.UtenteGiovane</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UtenteGiovane</servlet-name>
    <url-pattern>/UtenteGiovane</url-pattern>
  </servlet-mapping>
</web-app>