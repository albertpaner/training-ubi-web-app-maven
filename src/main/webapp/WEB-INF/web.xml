<web-app>
	<display-name>UbiWebApp</display-name>

	<servlet>
		<servlet-name>RegistrationServlet</servlet-name>
		<display-name>RegistrationServlet</display-name>
		<servlet-class>servlets.RegistrationServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>RegistrationServlet</servlet-name>
		<url-pattern>/register</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>LoginServlet</servlet-name>
		<display-name>LoginServlet</display-name>
		<servlet-class>servlets.LoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LoginServlet</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>DistributeEvaluatorsServlet</servlet-name>
		<display-name>DistributeEvaluatorsServlet</display-name>
		<servlet-class>servlets.DistributeEvaluatorsServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DistributeEvaluatorsServlet</servlet-name>
		<url-pattern>/distribute_evaluators</url-pattern>
	</servlet-mapping>



	<welcome-file-list>
		<welcome-file>registration.jsp</welcome-file>
		<welcome-file>login.jsp</welcome-file>
		<welcome-file>distribute_evaluators.jsp</welcome-file>
	</welcome-file-list>

</web-app>