<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
	<display-name>UbiWebApp</display-name>

	<servlet>
		<servlet-name>DistributeEvaluatorsServlet</servlet-name>
		<display-name>DistributeEvaluatorsServlet</display-name>
		<servlet-class>servlets.DistributeEvaluatorsServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>LoginServlet</servlet-name>
		<display-name>LoginServlet</display-name>
		<servlet-class>servlets.LoginServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>RegistrationServlet</servlet-name>
		<display-name>RegistrationServlet</display-name>
		<servlet-class>servlets.RegistrationServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DistributeEvaluatorsServlet</servlet-name>
		<url-pattern>/distributeEvaluators</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>LoginServlet</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>RegistrationServlet</servlet-name>
		<url-pattern>/RegistrationServlet</url-pattern>
	</servlet-mapping>

	<welcome-file-list>
		<welcome-file>distribute_evaluators.jsp</welcome-file>
		<welcome-file>registration.jsp</welcome-file>
		<welcome-file>login.jsp</welcome-file>
	</welcome-file-list>

</web-app>